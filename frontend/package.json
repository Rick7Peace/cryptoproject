{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "npm i rollup@3.29.4 --no-save --force && npm i @rollup/rollup-linux-x64-gnu@3.29.4 --no-save --force --no-optional || echo 'Platform-specific rollup binary not installed - using JS fallback'",
    "build": "react-router build",
    "postbuild": "node -e \"require('fs').copyFileSync('public/_redirects', 'build/client/_redirects')\"",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "clean-rebuild": "rm -rf node_modules package-lock.json && npm install && npm run build"
  },
  "dependencies": {
    "@react-router/node": "^7.4.1",
    "@react-router/serve": "^7.4.1",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.4.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.4.1",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "overrides": {
    "rollup": "3.29.4"
  }
}